{
  "name": "@hsl/jore4-test-db-manager",
  "version": "1.0.0",
  "description": "DB manager for Jore4 testing",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn rollup --config rollup.config.mjs",
    "seed": "node dist/seed.js",
    "seed2": "node dist/seed2.js",
    "clean": "rm -rf ts-dist && rm -rf dist && rm -f tsconfig.tsbuildinfo && rm -rf .rollup.cache",
    "ts:check": "yarn tsc --noEmit",
    "ts:build": "yarn tsc --build"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "cross-fetch": "^4.0.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.0",
    "pg": "^8.11.2",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/pg": "^8.10.2"
  }
}
