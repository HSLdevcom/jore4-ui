{
  "name": "@hsl/jore4-test-db-manager",
  "version": "1.0.0",
  "description": "DB manager for Jore4 testing",
  "private": true,
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/CypressSpecExports": {
      "default": "./dist/CypressSpecExports.js",
      "types": "./dist/types/CypressSpecExports.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "yarn clean && yarn rollup --config rollup.config.mjs",
    "seed": "yarn seed:timetables && yarn seed:stops",
    "seed:timetables": "node -e 'require(`./dist/index.js`).seedTimetables()'",
    "seed:stops": "node -e 'require(`./dist/index.js`).seedStopRegistry()'",
    "seed:terminals": "node -e 'require(`./dist/index.js`).seedStopRegistryTerminals()'",
    "clean": "rm -rf ts-dist && rm -rf dist && rm -f tsconfig.tsbuildinfo && rm -rf .rollup.cache",
    "ts:check": "yarn tsc --noEmit",
    "ts:build": "yarn tsc --build"
  },
  "dependencies": {
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/geojson": "^7946.0.16",
    "@types/luxon": "^3.7.1",
    "@types/pg": "^8.16.0",
    "rollup": "^4.55.1",
    "ts-node": "^10.9.2"
  }
}
