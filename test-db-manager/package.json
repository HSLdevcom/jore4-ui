{
  "name": "@hsl/jore4-test-db-manager",
  "version": "1.0.0",
  "description": "DB manager for Jore4 testing",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn rollup --config rollup.config.js",
    "seed": "node dist/seed.js",
    "clean": "rm -rf ts-dist && rm -rf dist && rm -f tsconfig.tsbuildinfo && rm -rf .rollup.cache",
    "ts:check": "yarn tsc --noEmit",
    "ts:build": "yarn tsc --build"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/geojson": "^7946.0.10",
    "cross-fetch": "^3.1.5",
    "graphql": "^15.5.3",
    "graphql-tag": "^2.12.6",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "pg": "^8.7.3",
    "rollup": "^2.75.5",
    "rollup-plugin-dts": "^4.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.6.5"
  }
}
